<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogFileController">
   <children>
      <JFXTextArea fx:id="taLog" layoutX="59.0" layoutY="95.0" prefHeight="211.0" prefWidth="490.0" />
      <Label fx:id="lblLog" layoutX="249.0" layoutY="36.0" text="LogFile" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="System Bold" size="26.0" />
         </font>
      </Label>
      <JFXButton fx:id="bntReadLog" layoutX="67.0" layoutY="340.0" onAction="#readLog" ripplerFill="#3f19ff" style="-fx-background-color: #2EFE9A;" text="Leer log">
         <effect>
            <Glow level="0.42" />
         </effect>
      </JFXButton>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="67.0" layoutY="22.0" opacity="0.39" stroke="BLACK" strokeType="INSIDE" width="608.0" />
      <JFXButton layoutX="169.0" layoutY="340.0" onAction="#orderByUser" style="-fx-background-color: #87CEEB;" text="Ordenar por usuario" />
      <JFXButton fx:id="bntOrdenarTipo" layoutX="328.0" layoutY="340.0" onAction="#orderByTipo" style="-fx-background-color: #87CEEB;" text="Ordenar por Fecha" />
